{
  "name": "figma-ui3-kit-svelte",
  "version": "0.4.0",
  "type": "module",
  "description": "A collection of Svelte 4 components that match Figma UI3 for building plugins",
  "svelte": "./src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "svelte-click-outside": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@storybook/addon-a11y": "^8.6.15",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-themes": "^8.6.15",
    "@storybook/svelte": "^8.6.14",
    "@storybook/svelte-webpack5": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "eslint": "^9.39.2",
    "eslint-plugin-svelte": "^3.14.0",
    "picomatch": "^4.0.3",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "storybook": "^8.6.14",
    "svelte": "^4.2.20",
    "svelte-check": "^4.3.6",
    "svelte-loader": "^3.2.4",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{js,svelte,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,svelte,css}\"",
    "check": "svelte-check --tsconfig ./jsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mariusroosendaal/figma-ui3-kit-svelte.git"
  },
  "keywords": [
    "figma",
    "plugin",
    "design-system",
    "ui-components",
    "svelte",
    "figma-ui3"
  ],
  "author": "Marius Roosendaal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariusroosendaal/figma-ui3-kit-svelte/issues"
  },
  "homepage": "https://github.com/mariusroosendaal/figma-ui3-kit-svelte#readme"
}
